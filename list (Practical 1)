print("-----------------------------------------------------------------------------------------")
lst.count(786)
print("-----------------------------------------------------------------------------------------")
lst.remove(786)
print(lst)
print("-----------------------------------------------------------------------------------------")

lst.insert([3][0],"hetu")
print(lst)
print("-----------------------------------------------------------------------------------------")
