    "def Factorial(n):\n",
    "    if n==0 or n ==1:\n",
    "        return 1\n",
    "    elif n<=0:\n",
    "        print('please enter positive number')\n",
    "        \n",
    "    else:\n",
    "        return n*Factorial(n-1)\n",
    "    \n",
    "n = int(input('Enter A number:'))    \n",
    "Factorial(n)"
